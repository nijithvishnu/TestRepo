                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="EntityNumber" type="EntityNumber">
                            <xs:annotation>
                                <xs:documentation>Entity number for the event to be reversed.</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="RequestingUserNumber" type="EntityNumber">
                            <xs:annotation>
                                <xs:documentation>The capturing user or systems entity number</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element maxOccurs="unbounded" name="PointsEventRequest">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="PointsEventTypeIdentifier" type="EventTypeIdentifier">
                                        <xs:annotation>
                                            <xs:documentation>The mnemonic of the event being reversed</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="PointsEventDate" type="xs:date">
                                        <xs:annotation>
                                            <xs:documentation>Date on which the points event took place</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="MatchPointsEventDate" type="xs:boolean">
                                        <xs:annotation>
                                            <xs:documentation>Specifies whether or not an exact date match is required for the Event Date. If false, then the event with closest date backwards from the given date will be reversed</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                </xs:sequence>
                                <xs:attribute name="correlationNumber" type="xs:long" use="required">
                                    <xs:annotation>
                                        <xs:documentation>An identifier for each reverse event request within the batch being passed to this operation and being unique within that batch. This id will be returned for the matching reverse event response.</xs:documentation>
                                    </xs:annotation>
                                </xs:attribute>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                    <xs:attribute name="schemaVersion" use="required">
                        <xs:simpleType>
                            <xs:restriction base="xs:float">
                                <xs:minInclusive value="1.0"/>
                                <xs:maxExclusive value="2.0"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:attribute>
                </xs:complexType>
            </xs:element>
        </xs:schema>
