            <xs:element name="OnlinePointsEventsCaptureRequestMessage">
                <xs:annotation>
                    <xs:documentation>A service to capture a number of points events for an entity, 
                all on the same policy, and on the same date.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="EntityNumber" type="EntityNumber">
                            <xs:annotation>
                                <xs:documentation>Entity number of the Vitality member who completed the event</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element maxOccurs="30" name="PointsEvent" type="PointsEvent"/>
                        <xs:element name="EventDate" type="xs:dateTime">
                            <xs:annotation>
                                <xs:documentation>The date on which the events were completed</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="PartnerBranch" type="EntityNumber">
                            <xs:annotation>
                                <xs:documentation>The partner branch at which the events were completed</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="Partner" type="EntityNumber">
                            <xs:annotation>
                                <xs:documentation>The partner organisation for the branch at which the event was completed.</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="CapturedBy" type="EntityNumber">
                            <xs:annotation>
                                <xs:documentation>Entity number of a system or human user who actually captures the event.</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                    <xs:attribute name="schemaVersion" use="required">
                        <xs:simpleType>
                            <xs:restriction base="xs:float">
                                <xs:minInclusive value="1.0"/>
                                <xs:maxExclusive value="2.0"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:attribute>
                </xs:complexType>
            </xs:element>
            <xs:complexType name="PointsEvent">
                <xs:sequence>
                    <xs:element name="Identifier" type="EventIdentifier">
                        <xs:annotation>
                            <xs:documentation>Event mnemonic.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="Score" type="xs:long">
                        <xs:annotation>
                            <xs:documentation>The points allocation for the event (affects points) if required.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="GeneralPurposeDate" type="xs:dateTime">
                        <xs:annotation>
                            <xs:documentation>For any other date besides the event date (Example: expected date of delivery for PREG events)</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:complexType>
        </xs:schema>
