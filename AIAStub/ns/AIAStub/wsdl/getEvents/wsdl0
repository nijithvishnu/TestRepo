        <xs:schema xmlns="http://schemas.discovery.co.za/vitality" xmlns:xs="http://www.w3.org/2001/XMLSchema" attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://schemas.discovery.co.za/vitality">
            <xs:complexType name="DisplayGroup">
                <xs:annotation>
                    <xs:documentation>Individual display groups. Each vitality event is
				grouped in event groups. This element describes the event group.</xs:documentation>
                </xs:annotation>
                <xs:sequence>
                    <xs:element name="Id" type="xs:int">
                        <xs:annotation>
                            <xs:documentation>Numeric identifier for display group</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="Order" type="xs:int">
                        <xs:annotation>
                            <xs:documentation>The display order of the display group</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="Description">
                        <xs:annotation>
                            <xs:documentation>The description of display group</xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:minLength value="0"/>
                                <xs:maxLength value="200"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element minOccurs="0" name="Identifier">
                        <xs:annotation>
                            <xs:documentation>String identifier for the display group</xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:minLength value="0"/>
                                <xs:maxLength value="60"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element minOccurs="0" name="ParentId" type="xs:int">
                        <xs:annotation>
                            <xs:documentation>Numeric identifier for display group parent id</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:complexType>
            <xs:complexType name="VitalityBusinessFaultResponse">
                <xs:annotation>
                    <xs:documentation>Fault describing a business exception state</xs:documentation>
                </xs:annotation>
                <xs:sequence>
                    <xs:element name="FaultCode" type="xs:long">
                        <xs:annotation>
                            <xs:documentation>Fault code associated with a business exception
						state</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="FaultMessage" type="xs:string">
