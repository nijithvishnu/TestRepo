            <xs:element name="InsertPolicyStatusRequestMessage">
                <xs:annotation>
                    <xs:documentation>Request to insert a new policy status.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="VitalityPolicyNumber" type="VitalityPolicyNumber">
                            <xs:annotation>
                                <xs:documentation>Vitality policy number.</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="VitalityStatus" type="xs:int">
                            <xs:annotation>
                                <xs:documentation>The status for the policy. This
							element gives status number/code 1,2,3,4 and
							5</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="ActionCode" type="xs:int">
                            <xs:annotation>
                                <xs:documentation>The action that causes the status to be created.
							Currently the possible action codes are
							1 - Status Calc,
							2 - Manual Override,
							3 - Status carry over,
							4 - Staff upgrade,
							5 - Corporate health status,
							6 - Default status,
							7 - General upgrade,
							8 - Highest earned status,
							9 - Reward status,
							10 - Locked in status,
							11 - Transferred status,</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="OriginCode" type="xs:int">
                            <xs:annotation>
                                <xs:documentation>Where does this new status code originate from?
							Currently the origin codes are
							1 - Points calc status
							2 - Partner program status
							3 - Vitality admin status
							4 - Diamond status
							5 - Corp Health status
							6 - Reward status
							7 - Transfer status</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="EffectiveFrom" type="xs:date">
                            <xs:annotation>
                                <xs:documentation>The date from which the Status will be
							in effect</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="EffectiveTo" type="xs:date">
                            <xs:annotation>
                                <xs:documentation>The date until which the Status will be
							in effect</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="RequestingUserNumber" type="EntityNumber">
                            <xs:annotation>
                                <xs:documentation>The capturing user or system’s entity number</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                    <xs:attribute name="schemaVersion" use="required">
                        <xs:simpleType>
                            <xs:restriction base="xs:float">
                                <xs:minInclusive value="1.0"/>
                                <xs:maxExclusive value="2.0"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:attribute>
                </xs:complexType>
            </xs:element>
        </xs:schema>
