                        <xs:annotation>
                            <xs:documentation>The surname of the member</xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:minLength value="0"/>
                                <xs:maxLength value="30"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element minOccurs="0" name="Gender" type="Gender">
                        <xs:annotation>
                            <xs:documentation>Gender of the member</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="Sex" type="Gender">
                        <xs:annotation>
                            <xs:documentation>Gender of the member</xs:documentation>
                        </xs:annotation>
                    </xs:element>
