                                <xs:enumeration value="PrimaryDriver"/>
                                <xs:enumeration value="SecondaryDriver"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element minOccurs="0" name="EffectivePeriod" type="Period"/>
                </xs:sequence>
            </xs:complexType>
