                        <xs:annotation>
                            <xs:documentation>No of times the event can occur</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="PlanValidation" type="EventTypePlanValidation">
                        <xs:annotation>
                            <xs:documentation>Validation,if any, on the event, like
						None,ExceptKEYCLUB</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="DisplayGroupId" type="xs:int">
                        <xs:annotation>
                            <xs:documentation>The id of the display group within which the
						element is to be displayed</xs:documentation>
                        </xs:annotation>
                    </xs:element>
