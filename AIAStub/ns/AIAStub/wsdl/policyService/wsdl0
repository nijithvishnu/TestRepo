        <xs:schema xmlns="http://schemas.discovery.co.za/vitality" xmlns:xs="http://www.w3.org/2001/XMLSchema" attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://schemas.discovery.co.za/vitality">
            <xs:element name="GetPolicyStatisticsResponseMessage">
                <xs:annotation>
                    <xs:documentation>This element is the Response message for querying vitality policy statistics.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:complexContent>
                        <xs:extension base="GetPolicyStatisticsResponse">
                            <xs:attribute name="schemaVersion" use="required">
                                <xs:simpleType>
                                    <xs:restriction base="xs:float">
                                        <xs:minInclusive value="1.0"/>
                                        <xs:maxExclusive value="2.0"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:attribute>
                        </xs:extension>
                    </xs:complexContent>
                </xs:complexType>
            </xs:element>
            <xs:complexType name="GetPolicyStatisticsResponse">
                <xs:annotation>
                    <xs:documentation>This element describes the response that is generated on the query for policy status</xs:documentation>
                </xs:annotation>
                <xs:sequence>
                    <xs:element name="PolicyStatistics" type="PolicyStatistics">
                        <xs:annotation>
                            <xs:documentation>This element describes the complete statistics of the vitality policy</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:complexType>
            <xs:complexType name="PolicyStatistics">
                <xs:annotation>
                    <xs:documentation>This element describes the complete statistics of the vitality policy</xs:documentation>
                </xs:annotation>
                <xs:sequence>
                    <xs:element name="PolicyNumber" type="VitalityPolicyNumber">
                        <xs:annotation>
                            <xs:documentation>The Vitality policy number for which the query was sent</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="SystemDate" type="xs:date">
                        <xs:annotation>
                            <xs:documentation>The date on which the policy statistics are to be known</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element maxOccurs="unbounded" name="Entity" type="Entity">
                        <xs:annotation>
                            <xs:documentation>The entities/members belonging to the policy</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="PointsEvents">
                        <xs:annotation>
                            <xs:documentation>The detailed description of the points and events for each entity on the policy. The points are calculated on occurrence of certain events on the policy</xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element maxOccurs="unbounded" name="Category" type="Category">
                                    <xs:annotation>
                                        <xs:documentation>The category/event that has occured in the policy. This is the top level event.</xs:documentation>
                                    </xs:annotation>
                                </xs:element>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="Points">
                        <xs:annotation>
                            <xs:documentation>The detailed description of overall points on the policy</xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="Points" type="xs:long">
                                    <xs:annotation>
                                        <xs:documentation>The points acquired on the policy</xs:documentation>
                                    </xs:annotation>
                                </xs:element>
                                <xs:element name="PointsToNextStatus" type="xs:long">
                                    <xs:annotation>
                                        <xs:documentation>The points that will be added when the next event occurs</xs:documentation>
                                    </xs:annotation>
                                </xs:element>
                                <xs:element name="RolePoints" type="RolePoints">
                                    <xs:annotation>
                                        <xs:documentation>The points for each individual entity in the policy</xs:documentation>
                                    </xs:annotation>
                                </xs:element>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="Status">
                        <xs:annotation>
                            <xs:documentation>The detailed description of overall status of the policy</xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                            <xs:complexContent>
                                <xs:extension base="Status">
                                    <xs:sequence>
                                        <xs:element name="ThresholdAdjustment" type="xs:long">
                                            <xs:annotation/>
                                        </xs:element>
                                    </xs:sequence>
                                </xs:extension>
                            </xs:complexContent>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="CarryOvers">
                        <xs:annotation>
                            <xs:documentation>This element describes the points that can be carried over to the next policy period</xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="Order" type="xs:int">
                                    <xs:annotation>
                                        <xs:documentation>Order of display for the carry over</xs:documentation>
                                    </xs:annotation>
                                </xs:element>
                                <xs:element name="Description">
                                    <xs:annotation>
                                        <xs:documentation>The description of carry over</xs:documentation>
                                    </xs:annotation>
                                    <xs:simpleType>
                                        <xs:restriction base="xs:string">
                                            <xs:minLength value="0"/>
                                            <xs:maxLength value="200"/>
                                        </xs:restriction>
                                    </xs:simpleType>
                                </xs:element>
                                <xs:element name="Identifier">
                                    <xs:annotation>
                                        <xs:documentation>This is text identifier for event group</xs:documentation>
                                    </xs:annotation>
                                    <xs:simpleType>
                                        <xs:restriction base="xs:string">
                                            <xs:minLength value="0"/>
                                            <xs:maxLength value="60"/>
                                        </xs:restriction>
                                    </xs:simpleType>
                                </xs:element>
                                <xs:element name="CurrentPeriod" type="CarryOver">
                                    <xs:annotation>
                                        <xs:documentation>The period for with the points are carried</xs:documentation>
                                    </xs:annotation>
                                </xs:element>
                                <xs:element name="NextPeriod" type="CarryOver">
                                    <xs:annotation>
                                        <xs:documentation>The next period over which the points will be carried</xs:documentation>
                                    </xs:annotation>
                                </xs:element>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element maxOccurs="unbounded" name="Benefit" type="Benefit">
                        <xs:annotation>
                            <xs:documentation>The benefits on the policy</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="StatusThresholds">
                        <xs:annotation>
                            <xs:documentation>Status of the vitality policy. This status is based on the points accumulated in the policy. The status will be like Blue, Bronze, Silver and Gold.</xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element maxOccurs="unbounded" name="Threshold" type="Threshold">
                                    <xs:annotation>
                                        <xs:documentation>This element describes the threshold, that is point range, for each status of the policy. The status will be like Blue, Bronze, Silver and Gold. This is based on the points accumulated in the policy</xs:documentation>
                                    </xs:annotation>
                                </xs:element>
                                <xs:element name="NumberOfAdults" type="xs:int">
                                    <xs:annotation>
                                        <xs:documentation>No of adult in the policy</xs:documentation>
                                    </xs:annotation>
                                </xs:element>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element maxOccurs="unbounded" name="DisplayGroup" type="DisplayGroup">
                        <xs:annotation>
                            <xs:documentation>Listing of display group, the event groups.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="InternalCompany" type="xs:int">
                        <xs:annotation>
                            <xs:documentation>The internal numeric code given for each company in discovery system</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="ReturnNumber" type="xs:int">
                        <xs:annotation>
                            <xs:documentation>Return code of the method call</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="ReturnMessage" type="xs:string">
                        <xs:annotation>
                            <xs:documentation>Description associated with the return code</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:complexType>
            <xs:complexType name="BaseCategory">
                <xs:annotation>
                    <xs:documentation>The base element for all the event/category</xs:documentation>
                </xs:annotation>
