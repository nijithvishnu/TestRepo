                                <xs:minLength value="0"/>
                                <xs:maxLength value="30"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                </xs:sequence>
            </xs:complexType>
            <xs:complexType name="EventType">
                <xs:annotation>
                    <xs:documentation>The details about the event type</xs:documentation>
                </xs:annotation>
                <xs:sequence>
