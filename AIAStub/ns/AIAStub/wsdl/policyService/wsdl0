                        <xs:annotation>
                            <xs:documentation>The points acquired as a result of event</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="EventCount" type="xs:long">
                        <xs:annotation>
                            <xs:documentation>The total no of event occured on the policy</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="RolePoints" type="RolePoints">
                        <xs:annotation>
                            <xs:documentation>The points for each individual entity in the policy</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:complexType>
            <xs:complexType name="Category">
                <xs:annotation>
                    <xs:documentation>The category/event that has occured in the policy. This is the top level event.</xs:documentation>
                </xs:annotation>
                <xs:complexContent>
                    <xs:extension base="BaseCategory">
                        <xs:sequence>
                            <xs:element maxOccurs="unbounded" name="SubCategory" type="SubCategory">
                                <xs:annotation>
                                    <xs:documentation>The category/event that has occured in the policy. This is the level two event.</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                        </xs:sequence>
                    </xs:extension>
                </xs:complexContent>
            </xs:complexType>
            <xs:complexType name="SubCategory">
                <xs:annotation>
                    <xs:documentation>The category/event that has occured in the policy. This is the level two event.</xs:documentation>
                </xs:annotation>
                <xs:complexContent>
                    <xs:extension base="BaseCategory">
                        <xs:sequence>
                            <xs:element maxOccurs="unbounded" name="DetailCategory" type="DetailCategory">
                                <xs:annotation>
                                    <xs:documentation>These is the actual event occuring on the policy, the lowest level in the event hierarchy. This is level 3 event</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                        </xs:sequence>
                    </xs:extension>
                </xs:complexContent>
            </xs:complexType>
            <xs:complexType name="DetailCategory">
                <xs:annotation>
                    <xs:documentation>These is the actual event occuring on the policy, the lowest level in the event hierarchy. This is level 3 event</xs:documentation>
                </xs:annotation>
                <xs:complexContent>
                    <xs:extension base="BaseCategory">
                        <xs:sequence>
                            <xs:element name="FrequencyValidation">
                                <xs:annotation>
                                    <xs:documentation>&gt;Frequency of event occuring in the policy, like OneAYear</xs:documentation>
