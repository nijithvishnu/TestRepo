            <xs:element name="GetPolicyStatisticsRequestMessage">
                <xs:annotation>
                    <xs:documentation>This element is the request message for querying vitality policy statistics.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:complexContent>
                        <xs:extension base="GetPolicyStatisticsRequest">
                            <xs:attribute name="schemaVersion" use="required">
                                <xs:simpleType>
                                    <xs:restriction base="xs:float">
                                        <xs:minInclusive value="1.0"/>
                                        <xs:maxExclusive value="2.0"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:attribute>
                        </xs:extension>
                    </xs:complexContent>
                </xs:complexType>
            </xs:element>
            <xs:complexType name="GetPolicyStatisticsRequest">
                <xs:annotation>
                    <xs:documentation>Please note that it is mandatory to include either vitalityPolicyNumber or entityNumber</xs:documentation>
                </xs:annotation>
                <xs:sequence>
                    <xs:choice>
                        <xs:element minOccurs="0" name="VitalityPolicyNumber" type="VitalityPolicyNumber">
                            <xs:annotation>
                                <xs:documentation>The policy statistics can be queries on Vitality Policy Number. This is one of the parameter to query the policy.</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="EntityNumber" type="EntityNumber">
                            <xs:annotation>
                                <xs:documentation>The policy statistics can be queries on Vitality Entity Number. This is one of the parameter to query the policy.</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:choice>
                    <xs:element name="Language">
                        <xs:annotation>
                            <xs:documentation>Language for the data in the xmls. For english pass E for Africaans A</xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:minLength value="0"/>
                                <xs:maxLength value="1"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element minOccurs="0" name="EffectiveDate" type="xs:date">
                        <xs:annotation>
                            <xs:documentation>"EffectiveDate" must be after vitality policy DOC. If no date is given the system date is considered</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="PerionAdjustment" type="xs:int">
                        <xs:annotation>
                            <xs:documentation>-1 should be passed to get last policy period and so on.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:sequence maxOccurs="unbounded" minOccurs="0">
                        <xs:any/>
                    </xs:sequence>
                </xs:sequence>
            </xs:complexType>
            <xs:simpleType name="NumericReference">
