            <xs:complexType name="EntityEventWithEventCapturer">
                <xs:annotation>
                    <xs:documentation>The event that occurred in the policy for the entity</xs:documentation>
                </xs:annotation>
                <xs:sequence>
                    <xs:element name="EventDate" type="xs:date">
                        <xs:annotation>
                            <xs:documentation>The date on which the event has occurred</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="Points" type="xs:long">
                        <xs:annotation>
                            <xs:documentation>The points acquired as a result of event.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="Status">
                        <xs:annotation>
                            <xs:documentation>The status of event, the enumeration values are listed below</xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:minLength value="0"/>
                                <xs:maxLength value="1"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element minOccurs="0" name="RejectionReason">
                        <xs:annotation>
                            <xs:documentation>The reason for rejection of event, if event was rejected</xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:minLength value="0"/>
                                <xs:maxLength value="200"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element minOccurs="0" name="RejectionReasonIdentifier">
                        <xs:annotation>
                            <xs:documentation>The unique identifier code for rejection reason.</xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:minLength value="0"/>
                                <xs:maxLength value="120"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="EventCaptureDate" type="xs:date">
                        <xs:annotation>
                            <xs:documentation>The date on which the event was captured</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="CapturerUserCd" type="EntityNumber">
                        <xs:annotation>
                            <xs:documentation>The Capturer Entity number of the Event.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="CapturerName">
                        <xs:annotation>
                            <xs:documentation>The Capturer name of the Event</xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:minLength value="0"/>
                                <xs:maxLength value="480"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element maxOccurs="unbounded" minOccurs="0" name="BoosterEventWithEventCapturer" type="BoosterEventWithEventCapturer">
                        <xs:annotation>
                            <xs:documentation>The booster event associated with the main event</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:complexType>
            <xs:complexType name="BoosterEventWithEventCapturer">
                <xs:annotation>
                    <xs:documentation>The booster event are child events associated with main event</xs:documentation>
                </xs:annotation>
                <xs:sequence>
                    <xs:element name="Mnemonic" type="EventTypeMnemonic">
                        <xs:annotation>
                            <xs:documentation>The category code OR Mnemonic associated with the event</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="Description">
                        <xs:annotation>
                            <xs:documentation>The description of the event</xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:minLength value="0"/>
                                <xs:maxLength value="150"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="EventDate" type="xs:date">
                        <xs:annotation>
                            <xs:documentation>The date on which the event has occurred</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="Points" type="xs:long">
                        <xs:annotation>
                            <xs:documentation>The points associated with the event</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:complexType>
            <xs:complexType name="CarryOverWithEventCapturer">
                <xs:annotation>
                    <xs:documentation>This element describes the points that can be carried over to the next policy period</xs:documentation>
                </xs:annotation>
                <xs:sequence>
                    <xs:element name="Points" type="xs:long">
                        <xs:annotation>
                            <xs:documentation>The points that can be carried over</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="PeriodStart" type="xs:date">
                        <xs:annotation>
                            <xs:documentation>Start date of the period</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="PeriodEnd" type="xs:date">
                        <xs:annotation>
                            <xs:documentation>End date of the period</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:complexType>
            <xs:complexType name="BenefitWithEventCapturer">
                <xs:annotation>
                    <xs:documentation>The benefit on the policy</xs:documentation>
                </xs:annotation>
                <xs:sequence>
                    <xs:element name="Type">
                        <xs:annotation>
                            <xs:documentation>The type of the benefit. There are pre-defined benifits in the system like Pick n Pay HealthyFood Benefit</xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:minLength value="0"/>
                                <xs:maxLength value="7"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="Description">
                        <xs:annotation>
                            <xs:documentation>The description of the benefit</xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:minLength value="0"/>
                                <xs:maxLength value="40"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element maxOccurs="unbounded" name="ComponentWithEventCapturer" type="ComponentWithEventCapturer">
                        <xs:annotation>
                            <xs:documentation>The benefit component. A benefit will have multiple component.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:complexType>
            <xs:complexType name="ComponentWithEventCapturer">
                <xs:annotation>
                    <xs:documentation>The benefit can have multiple or single component. The example of benefit component are HealthyFood Credit Card</xs:documentation>
                </xs:annotation>
                <xs:sequence>
                    <xs:element name="ComponentId">
                        <xs:annotation>
                            <xs:documentation>Unique idetifier/code for a component</xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:minLength value="0"/>
                                <xs:maxLength value="7"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="Description">
                        <xs:annotation>
                            <xs:documentation>Description of the component</xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:minLength value="0"/>
                                <xs:maxLength value="40"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element maxOccurs="unbounded" name="EntityComponentWithEventCapturer" type="EntityComponentWithEventCapturer">
                        <xs:annotation>
                            <xs:documentation>The component contributed to each entity in the policy</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:complexType>
            <xs:complexType name="EntityComponentWithEventCapturer">
                <xs:annotation>
                    <xs:documentation>The component contributed to each entity in the policy</xs:documentation>
                </xs:annotation>
                <xs:sequence>
                    <xs:element name="EntityNumber" type="EntityNumber">
                        <xs:annotation>
                            <xs:documentation>The entity number of the entity</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="InactiveDate" nillable="true" type="xs:date">
                        <xs:annotation>
                            <xs:documentation>If the entity is in-activated, then the date when it was in-activated</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="Role">
                        <xs:annotation>
                            <xs:documentation>Role of the entity</xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:minLength value="0"/>
                                <xs:maxLength value="2"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                </xs:sequence>
            </xs:complexType>
        </xs:schema>
