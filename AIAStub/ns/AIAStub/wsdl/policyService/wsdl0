                                    <xs:restriction base="xs:float">
                                        <xs:minInclusive value="1.0"/>
                                        <xs:maxExclusive value="2.0"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:attribute>
                        </xs:extension>
                    </xs:complexContent>
                </xs:complexType>
            </xs:element>
            <xs:complexType name="RecreatePolicyRequest">
                <xs:annotation>
                    <xs:documentation>The container type for the policy recreate request.</xs:documentation>
                </xs:annotation>
                <xs:sequence>
                    <xs:element name="Vendor">
                        <xs:annotation>
                            <xs:documentation>An identifier for the Vitality vendor for which
						this policy is being created.</xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:maxLength value="64"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="Policy">
                        <xs:annotation>
                            <xs:documentation>The vitality policy to be created.</xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:choice>
                                    <xs:element name="VitalityPolicyNumber" type="VitalityPolicyNumber">
                                        <xs:annotation>
                                            <xs:documentation>The Vitality policy for which this
										product
										update is being made.</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="ExternalVitalityPolicyReference">
                                        <xs:annotation>
                                            <xs:documentation>External policy reference
										representing this
										vitality policy in
										the calling system. Note that this
										relationship is considered
										immutable.</xs:documentation>
                                        </xs:annotation>
                                        <xs:simpleType>
                                            <xs:restriction base="xs:string">
                                                <xs:maxLength value="50"/>
                                            </xs:restriction>
                                        </xs:simpleType>
                                    </xs:element>
                                </xs:choice>
                                <xs:element maxOccurs="unbounded" minOccurs="0" name="PolicyHolder" type="PolicyHolder">
                                    <xs:annotation>
                                        <xs:documentation>The holder of this policy.</xs:documentation>
                                    </xs:annotation>
                                </xs:element>
                                <xs:element maxOccurs="unbounded" name="Product" type="PolicyProductType">
                                    <xs:annotation>
                                        <xs:documentation>The details of the Vitality product
									for the
									policy.</xs:documentation>
                                    </xs:annotation>
                                </xs:element>
                                <xs:element maxOccurs="unbounded" name="Member" type="PolicyMemberType">
                                    <xs:annotation>
                                        <xs:documentation>Members to be associated to the
									policy.</xs:documentation>
                                    </xs:annotation>
                                </xs:element>
                                <xs:element maxOccurs="unbounded" minOccurs="0" name="PolicyPeriodFormula" type="PolicyPeriodFormulaType">
                                    <xs:annotation>
                                        <xs:documentation>If not provided there needs to be a
									rule
									configured for the vendor or
									the group.</xs:documentation>
                                    </xs:annotation>
                                </xs:element>
                                <xs:element maxOccurs="unbounded" name="PolicyState" type="PolicyState">
                                    <xs:annotation>
                                        <xs:documentation>Period(s) for which policy state is
									defined.</xs:documentation>
                                    </xs:annotation>
                                </xs:element>
                                <xs:element maxOccurs="unbounded" minOccurs="0" name="Group" type="PolicyGroupType">
                                    <xs:annotation>
                                        <xs:documentation>The type for group(s) associated
									with this
									policy.</xs:documentation>
                                    </xs:annotation>
                                </xs:element>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:any maxOccurs="unbounded" minOccurs="0" namespace="##any"/>
                </xs:sequence>
            </xs:complexType>
        </xs:schema>
