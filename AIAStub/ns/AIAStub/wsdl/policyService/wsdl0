                                <xs:element minOccurs="0" name="BenefitType">
                                    <xs:simpleType>
                                        <xs:restriction base="xs:string">
                                            <xs:maxLength value="64"/>
                                        </xs:restriction>
                                    </xs:simpleType>
                                </xs:element>
                                <xs:element name="EffectivePeriod" type="Period"/>
                            </xs:sequence>
                            <xs:attribute name="action">
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:enumeration value="Update"/>
                                        <xs:enumeration value="Delete"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:attribute>
                        </xs:complexType>
                    </xs:element>
                    <xs:element minOccurs="0" name="isInitiatingPolicy" type="xs:boolean">
                        <xs:annotation>
                            <xs:documentation>Assumed false if not supplied.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element maxOccurs="unbounded" name="RolePeriod">
                        <xs:complexType>
                            <xs:complexContent>
                                <xs:extension base="RolePeriod">
                                    <xs:attribute name="action">
                                        <xs:simpleType>
                                            <xs:restriction base="xs:string">
                                                <xs:enumeration value="Update"/>
                                                <xs:enumeration value="Delete"/>
                                            </xs:restriction>
                                        </xs:simpleType>
                                    </xs:attribute>
                                </xs:extension>
                            </xs:complexContent>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:complexType>
            <xs:complexType name="RolePeriod">
                <xs:sequence>
                    <xs:element minOccurs="0" name="MemberRole">
                        <xs:annotation>
                            <xs:documentation>The role of the member on the policy for a
						time
						period.</xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:maxLength value="64"/>
